@using Equipment.Models
@model List<TrackServiceHistoryPart>

@if(Model.Count==0)
{
    <div>Деталей нет</div>

}
else
{
    <table>
        <tr>
            <th>Код запчасти</th>
            <th>Количество</th>
            <th>Цена</th>
        </tr>
        @foreach (TrackServiceHistoryPart item in Model)
    {
        <tr>
            <td>@item.intMasterPartId</td>
            <td>@item.quantity</td>
            <td>@item.unit_cost</td>
        </tr>
    }
    </table>
}
