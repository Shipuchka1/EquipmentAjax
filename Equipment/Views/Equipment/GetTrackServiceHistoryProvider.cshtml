@using Equipment.Models
@model List<TrackServiceHistoryProvider>

@if(Model.Count==0)
{
    <div>Данной информации нет</div>
}
else
{
    <table>
        <tr>
            <th>ФИО</th>
            <th>Время</th>
        </tr>
        @foreach (TrackServiceHistoryProvider item in Model)
        {
        <tr>
            <td>@item.TablesSupplier.strSupplierName</td>
            <td>@item.intRepairLabor</td>
        </tr>
        }
    </table>
}
